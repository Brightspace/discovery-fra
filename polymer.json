{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-view404.html",
    "src/my-new-view.html",
    "src/search-page.html",
    "src/redux/redux-mixin.html",
    "src/landing-view.html",
