{
  "entrypoint": "index.html",
  "shell": "src/discovery-app.html",
  "fragments": [
    "src/discovery-home.html",
    "src/discovery-404.html",
    "src/discovery-course.html",
    "src/discovery-search.html",
    "src/discovery-manage.html"
  ],
  "sources": [
    "images/**/*",
    "src/**/*"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "!bower_components/webcomponentsjs/gulpfile.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
    "preset": "es5-bundled",
    "addServiceWorker": false
  }]
}
