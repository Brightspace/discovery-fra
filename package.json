{
  "name": "discovery",
  "appId": "urn:d2l:fra:class:discovery",
  "description": "Discovery",
  "version": "3.1.2",
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "repository": "git@github.com:Brightspace/discovery-fra.git",
  "scripts": {
    "lint": "npm run lint:javascript",
    "lint:javascript": "eslint src --ext js",
    "test": "wct test --skip-plugin sauce --npm",
    "test:integration": "polymer build # test that psk builds without error with the CLI",
    "test:polymer:sauce": "wct test --skip-plugin local --npm",
    "build": "polymer build && mv ./build/es5-bundled/* ./build && rmdir \"./build/es5-bundled\"",
    "build:debug": "polymer build --name debug && mv ./build/debug/* ./build && rmdir ./build/debug",
    "start": "npm run build && npm run frau:build-config && npm run frau:resolve",
    "start:debug": "npm run build:debug && npm run frau:build-config && npm run frau:resolve",
    "ifrautoaster": "ifrautoaster --config ./ifrautoaster-config.json",
    "frau:build-config": "frau-appconfig-builder",
    "frau:resolve": "frau-local-appresolver",
    "frau:publish": "frau-publisher",
    "frau:release": "npm run frau:publish",
    "install-attest": "npm install --no-save attest",
    "buildDev": "npx gulp buildDev",
    "buildDev:noWatch": "npx gulp buildDevNoWatch",
    "buildDev:custom": "npx gulp buildDevCustom",
    "buildDev:customNoWatch": "npx gulp buildDevCustomNoWatch"
  },
  "dependencies": {
    "@brightspace-ui-labs/facet-filter-sort": "^4",
    "@brightspace-ui-labs/pagination": "^1",
    "@brightspace-ui/core": "^1",
    "@polymer/app-route": "^3.0.0-pre.15",
    "@polymer/iron-pages": "^3.0.0-pre.15",
    "@polymer/iron-resizable-behavior": "^3.0.0",
    "@polymer/polymer": "^3",
    "d2l-activities": "github:BrightspaceHypermediaComponents/activities#semver:^3",
    "d2l-fetch": "github:Brightspace/d2l-fetch#semver:^2",
    "d2l-fetch-auth": "^1",
    "d2l-fetch-dedupe": "^1.3.0",
    "d2l-hypermedia-constants": "^6",
    "d2l-localize-behavior": "github:BrightspaceUI/localize-behavior#semver:^2",
    "d2l-offscreen": "github:BrightspaceUI/offscreen#semver:^4",
    "d2l-organization-hm-behavior": "github:Brightspace/organization-hm-behavior#semver:^3",
    "d2l-organizations": "github:BrightspaceHypermediaComponents/organizations#semver:^5",
    "d2l-typography": "github:BrightspaceUI/typography#semver:^7",
    "dompurify": "^2.2.7",
    "fastdom": "^1.0.10",
    "lit-element-router": "^2.0.3",
    "promise-polyfill": "8.1.0",
    "siren-parser": "^8.4.0",
    "siren-sdk": "github:BrightspaceHypermediaComponents/siren-sdk#semver:^1",
    "url-polyfill": "^1.1.12",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@d2l/ifrautoaster": "^1.1.9",
    "@polymer/iron-component-page": "^4.0.1",
    "@polymer/iron-test-helpers": "^3.0.0-pre.18",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "babel-eslint": "^10.0.1",
    "bower": "^1.8.12",
    "death": "^1.1.0",
    "del": "^6.0.0",
    "depd": "^2.0.0",
    "eslint": "^7.23.0",
    "eslint-config-brightspace": "^0.4.0",
    "eslint-plugin-html": "^6.1.2",
    "frau-appconfig-builder": "^1.3.0",
    "frau-local-appresolver": "^1.0.0",
    "frau-publisher": "^2.9.0",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.3.0",
    "gulp-ejs": "^5.1.0",
    "gulp-rename": "^1.2.3",
    "merge-stream": "^1.0.1",
    "require-dir": "^1.2.0",
    "wct-browser-legacy": "^1.0.2",
    "wct-none": "^0.1.1",
    "web-component-tester": "^6.9.2"
  },
  "engines": {
    "node": ">=8.0"
  },
  "config": {
    "frauAppConfigBuilder": {
      "appFile": "index.html",
      "dist": "./build",
      "envVar": "CI",
      "loader": "iframe",
      "port": 3000
    },
    "frauLocalAppResolver": {
      "appClass": "urn:d2l:fra:class:discovery",
      "dist": "./build",
      "port": 3000
    },
    "frauPublisher": {
      "files": "./build/**",
      "moduleType": "app",
      "targetDirectory": "discovery",
      "devTagVar": "DEV_VER",
      "versionVar": "PACKAGE_VER"
    }
  }
}
